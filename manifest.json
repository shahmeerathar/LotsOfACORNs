{
  "name": "LotsOfACORNs",
  "description": "You're rich",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [ "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/numeral.min.js", "/js/richify.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/quercus-logo16.png",
      "32": "/images/quercus-logo32.png",
      "48": "/images/quercus-logo48.png",
      "128": "/images/quercus-logo128.png"
    }
  },
  "icons": {
    "16": "/images/quercus-logo16.png",
    "32": "/images/quercus-logo32.png",
    "48": "/images/quercus-logo48.png",
    "128": "/images/quercus-logo128.png"
  }
}
